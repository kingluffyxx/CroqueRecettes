{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    {
      "src": "/api/index.php",
      "dest": "/api/index.php"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/bundles/(.*)",
      "dest": "/public/bundles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "prod",
    "APP_DEBUG": "false",
    "VERCEL": "1",
    "SYMFONY_CACHE_DIR": "/tmp/symfony_cache",
    "SYMFONY_LOG_DIR": "/tmp/symfony_logs"
  }
}