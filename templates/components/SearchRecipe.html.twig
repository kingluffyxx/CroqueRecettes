<div {{ attributes }}>
    <label class="input">
			<svg class="opacity-50 h-[1em]" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 24 24">
				<g stroke-linejoin="round" stroke-linecap="round" stroke-width="2.5" fill="none" stroke="currentColor">
					<circle cx="11" cy="11" r="8"></circle>
					<path d="m21 21-4.3-4.3"></path>
				</g>
			</svg>
			<input type="search" class="focus:ring-0 w-full" placeholder="Search"/>
		</label>
		<label for="default-search" class="sr-only mb-2 font-medium text-gray-900 text-sm">Search</label>

    <ul>
        {% for recipe in this.recipes %}
            <li>{{ recipe.title }}</li>
        {% endfor %}
    </ul>
</div>